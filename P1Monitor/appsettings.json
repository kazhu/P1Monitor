{
  "DsmrReader": {
    "Host": "<name or address of the device>", // for example: 192.168.0.35
    "Port": 2323,
    "BufferSize": 4096  // needs to be large enough to hold the entire telegram + some extra
  },
  "InfluxDb": {
    "BaseUrl": "<name or address of the InfluxDb>", // for example: http://192.168.34.6:8086/
    "Token": "<token which has write access to the bucket>",
    "Organization": "<organization>",
    "Bucket": "<bucket>"
  },
  "ObisMapping": {
    "DeviceName": "EON_HU_SX631",
    "MappingFile": "obismappings.json"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}